{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@app/*": ["./src/*"],
      "*": [
        "node_modules/*"
      ]
    }
  },
  "include": [
    "src/**/*",
    "prisma/**/*"
  ]
}